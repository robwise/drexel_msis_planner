- content_for :title, 'Plans'
- content_for :description, "A list of the user's degree plans."
.col-xs-12.col-md-8.col-md-offset-2
  %h1 My Plans
  %ul.list-group
    = render @user.plans
.col-xs-4.col-xs-offset-4
  %a.btn.btn-primary.center-block{ href: new_user_plan_path(@user) }
    = glyphicon("plus") + " New Plan"
